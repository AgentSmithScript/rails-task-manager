<h1>My tasks</h1>

<ul>
  <div class="form-check">
  <% @tasks.each do |task| %>
    <li>
      <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
      <%= link_to task.title, task_path(task) %>
    </li>
  </div>
  <% end %>
</ul>

<%= link_to "Add Task", new_task_path %>
